{
    "name": "do-you-know-there",
    "description": "A game to test your knowledge of an area.",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "npm run build:images && next dev",
        "build": "npm run build:images && next build",
        "start": "npm run build:images && next start",
        "lint": "next lint",
        "format:check": "prettier --list-different '.'",
        "format:fix": "prettier --write '.'",
        "build:images": "ts-node/esm scripts/buildImages.ts"
    },
    "dependencies": {
        "@vercel/analytics": "^1.1.1",
        "leaflet": "^1.9.4",
        "next": "^14.0.1",
        "react": "^18",
        "react-dom": "^18",
        "react-leaflet": "^4.2.1"
    },
    "devDependencies": {
        "@types/leaflet": "^1.9.7",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10",
        "eslint": "^8",
        "eslint-config-next": "14.0.0",
        "postcss": "^8",
        "prettier": "^3.0.3",
        "sharp": "^0.32.6",
        "tailwindcss": "^3",
        "ts-node": "^10.9.1",
        "typescript": "^5"
    }
}
